summary(lm_rs214) %$% r.squared # 6% variance explained
summary(lm_custom) %$% r.squared # 20% variance explained
residuals(lm_custom) %>% shapiro.test # normal-ish p=0.03
residuals(lm_rs214) %>% shapiro.test # normal p=0.50
